    "@rollup/plugin-replace": "^2.3.1",
    "@rollup/plugin-typescript": "^3.0.0",
    "autoprefixer": "^9.7.4",
    "cssnano": "^4.1.10",
    "jest": "^25.1.0",
    "postcss-easy-import": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.9.0",
    "rollup": "^1.31.1",
    "rollup-plugin-postcss": "^2.1.1",
    "rollup-plugin-terser": "^5.2.0",
    "speakingurl": "^14.0.1",
    "stylus": "^0.54.7",
    "tiny-json-http": "^7.1.2",
